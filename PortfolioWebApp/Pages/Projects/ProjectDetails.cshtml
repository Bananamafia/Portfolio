@page
@model PortfolioWebApp.Pages.Projects.ProjectDetailsModel
@{
    ViewData["Title"] = Model.selectedProject.Title;
}



<h1>@Model.selectedProject.Title</h1>
<h5 class="text-muted font-weight-lighter">@Model.selectedProject.StartingDate.ToShortDateString() - @Model.selectedProject.EndingDate.ToShortDateString()</h5>

<p>@Model.selectedProject.Description</p>

<h4>Verwendete Tools:</h4>



<div id="carousel" class="carousel slide bg-dark" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carousel" data-slide-to="0" class="active"></li>
        <li data-target="#carousel" data-slide-to="1"></li>
        <li data-target="#carousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/Images/SampleImage.png" class="d-block mx-auto">
        </div>
        <div class="carousel-item">
            <img src="~/Images/SampleImage.png" class="d-block mx-auto">
        </div>
        <div class="carousel-item">
            <img src="~/Images/SampleImage.png" class="d-block mx-auto">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carousel" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

